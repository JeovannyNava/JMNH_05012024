                                <a href="/Account/Logout" class="dropdown-item notify-item">
                                    <i class="fe-log-out"></i>
                                    <span>Salir</span>
                                </a>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- ========== Topbar End ========== -->
            <div class="content">

                <!-- Start Content-->
                <div class="container-fluid">
                    @RenderBody()
                </div> <!-- container -->
            </div> <!-- content -->
        </div>

    </div>
